<%= simple_form_for(@worker) do |f| %>
  <%= f.input :email %>
  <%= f.input :name %>
  <%= f.input :alias %>
  <%= f.input :mobile_number %>
  <%= f.input :description %>
  <%= f.input :photo_id_front %>
  <%= f.input :photo_id_back %>
  <%= f.input :id_selfie %>
  <%= f.input :id_type %>
  <%= f.input :id_valid %>
  <%= f.input :payment_link %>
  <%= f.input :payment_qr %>
  <%= f.input :is_archived %>
  <%= f.submit %>
<% end %>
